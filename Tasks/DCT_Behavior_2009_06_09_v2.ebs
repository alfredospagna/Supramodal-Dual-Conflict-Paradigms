	'''''''''''''''''''''''''''''''''''''''''''''
	' InLine - InLineCalculateRT1000 BEGIN
	'''''''''''''''''''''''''''''''''''''''''''''
	Dim i as Integer 'generic counter variable
Dim nClicks As Integer 'number of responses
Dim kbdMask As InputMask
Dim kbdRespColl As ResponseDataCollection
Dim nTarget1Response As Integer 'number of responses for target 1
Dim nTarget2Response As Integer 'number of responses for target 2
Dim correctResponstT1 As String
Dim correctResponstT2 As String

Dim allowResponstT111 As String
Dim allowResponstT112 As String
Dim allowResponstT121 As String
Dim allowResponstT122 As String

Dim allowResponstT211 As String
Dim allowResponstT212 As String
Dim allowResponstT221 As String
Dim allowResponstT222 As String

If (c.GetAttrib("TaskOrder") = "UDLR") Then
	allowResponstT111 = "a"
	allowResponstT112 = "A"
	allowResponstT121 = "z"
	allowResponstT122 = "Z"

	allowResponstT211 = "{,}"
	allowResponstT212 = "{<}"
	allowResponstT221 = "{.}"
	allowResponstT222 = "{>}"
End If

If (c.GetAttrib("TaskOrder") = "LRUD") Then
	allowResponstT111 = "{,}"
	allowResponstT112 = "{<}"
	allowResponstT121 = "{.}"
	allowResponstT122 = "{>}"

	allowResponstT211 = "a"
	allowResponstT212 = "A"
	allowResponstT221 = "z"
	allowResponstT222 = "Z"
End If

nClicks = SlideTarget1000T1.InputMasks(1).Responses.Count

'total response number <= 4
c.SetAttrib "nResponse", nClicks

'examine the RTs for each kbd response by working with
'the kbdResponseData collection returned by the InputMask

'get the InputMask associated with the keyboard response
Set kbdRespColl = SlideTarget1000T1.InputMasks(1).Responses

'save RTs for each keyboard response (from start of trial)
For i = 1 to nClicks
   Select case i
        case 1
		    c.SetAttrib "RTime1", kbdRespColl(i).RTTime
        case 2
		    c.SetAttrib "RTime2", kbdRespColl(i).RTTime
        case 3
		    c.SetAttrib "RTime3", kbdRespColl(i).RTTime
        case 4
		    c.SetAttrib "RTime4", kbdRespColl(i).RTTime
    End Select
Next i
	
nTarget1Response = 0
nTarget2Response = 0
'calculate RTs
If nClicks >= 1 Then
	For i=1 to nClicks
	    If (kbdRespColl(i).RESP = allowResponstT111 OR kbdRespColl(i).RESP = allowResponstT112) AND (nTarget1Response = 0)  Then
		    c.SetAttrib "Target1RT", (kbdRespColl(i).RTTime - SlideTarget1000T1.OnsetTime)
			If kbdRespColl(i).RESP = c.GetAttrib("CorrectAnswerT1") Then
                c.SetAttrib "Target1ACC", 1
			Else
			    c.SetAttrib "Target1ACC", 0
			End If   
			nTarget1Response = 1 
		ElseIf (kbdRespColl(i).RESP = allowResponstT121 OR kbdRespColl(i).RESP = allowResponstT122) AND (nTarget1Response = 0) Then 
		    c.SetAttrib "Target1RT", (kbdRespColl(i).RTTime - SlideTarget1000T1.OnsetTime)
			If kbdRespColl(i).RESP = c.GetAttrib("CorrectAnswerT1") Then
                c.SetAttrib "Target1ACC", 1
			Else
			    c.SetAttrib "Target1ACC", 0
			End If   
			nTarget1Response = 1
		ElseIf (kbdRespColl(i).RESP = allowResponstT211 OR kbdRespColl(i).RESP = allowResponstT212) AND (nTarget2Response = 0) Then 
		    c.SetAttrib "Target2RT", (kbdRespColl(i).RTTime - SlideTarget1000T2.OnsetTime)
			If kbdRespColl(i).RESP = c.GetAttrib("CorrectAnswerT2") Then
                c.SetAttrib "Target2ACC", 1
			Else
			    c.SetAttrib "Target2ACC", 0
			End If   
			nTarget2Response = 1
		ElseIf (kbdRespColl(i).RESP = allowResponstT221 OR kbdRespColl(i).RESP = allowResponstT222) AND (nTarget2Response = 0) Then 
		    c.SetAttrib "Target2RT", (kbdRespColl(i).RTTime - SlideTarget1000T2.OnsetTime)
			If kbdRespColl(i).RESP = c.GetAttrib("CorrectAnswerT2") Then
                c.SetAttrib "Target2ACC", 1
			Else
			    c.SetAttrib "Target2ACC", 0
			End If   
			nTarget2Response = 1
		End If
	Next i
End If
	'''''''''''''''''''''''''''''''''''''''''''''
	' InLine - InLineCalculateRT1000 END
	'''''''''''''''''''''''''''''''''''''''''''''
	
	c.SetAttrib "SlideFixationITI1.OnsetTime", SlideFixationITI1.OnsetTime
	c.SetAttrib "SlideFixationITI1.Duration", SlideFixationITI1.Duration
	c.SetAttrib "SlideFixationITI1.StartTime", SlideFixationITI1.StartTime
	
