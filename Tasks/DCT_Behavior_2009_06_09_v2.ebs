	c.SetAttrib "SlideTarget1000T1.Duration", SlideTarget1000T1.Duration
	c.SetAttrib "SlideTarget1000T1.StartTime", SlideTarget1000T1.StartTime
	c.SetAttrib "SlideTarget1000T1.RTTime", SlideTarget1000T1.RTTime
	c.SetAttrib "SlideTarget1000T1.ACC", SlideTarget1000T1.ACC
	c.SetAttrib "SlideTarget1000T1.RT", SlideTarget1000T1.RT
	c.SetAttrib "SlideTarget1000T1.RESP", SlideTarget1000T1.RESP
	c.SetAttrib "SlideTarget1000T1.CRESP", SlideTarget1000T1.CRESP
	c.SetAttrib "SlideTarget1000T1.Tag", SlideTarget1000T1.Tag
	
	c.SetAttrib "SlideTarget1000T1ToT2.OnsetTime", SlideTarget1000T1ToT2.OnsetTime
	c.SetAttrib "SlideTarget1000T1ToT2.Duration", SlideTarget1000T1ToT2.Duration
	c.SetAttrib "SlideTarget1000T1ToT2.StartTime", SlideTarget1000T1ToT2.StartTime
	
	c.SetAttrib "SlideTarget1000T2.OnsetTime", SlideTarget1000T2.OnsetTime
	c.SetAttrib "SlideTarget1000T2.Duration", SlideTarget1000T2.Duration
	c.SetAttrib "SlideTarget1000T2.StartTime", SlideTarget1000T2.StartTime
	
	c.SetAttrib "SlideFixationITI2.OnsetTime", SlideFixationITI2.OnsetTime
	c.SetAttrib "SlideFixationITI2.Duration", SlideFixationITI2.Duration
	c.SetAttrib "SlideFixationITI2.StartTime", SlideFixationITI2.StartTime
	

	c.Log
